{
  "name": "export-code-context",
  "displayName": "Export Code Context",
  "description": "Easily export the code from a folder or file.",
  "version": "1.0.0",
  "publisher": "rahathasan",
  "engines": {
    "vscode": "^1.105.0"
  },
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/rahathasan5773/export-code-context.git"
  },
  "homepage": "https://github.com/rahathasan5773/export-code-context",
  "categories": [
    "Other",
    "Formatters"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "export-code-context.universalAction",
        "title": "Code Context Action"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "export-code-context.universalAction",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}